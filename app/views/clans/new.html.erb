<section>
  <header>
    <h1>Found a Clan</h1>
  </header>

  <%= form_with model: @clan do |form| %>
    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div class="field">
      <%= form.label :description %>
      <%= form.text_area :description %>
    </div>
    <div class="actions">
      <%= form.submit "Create" %>
    </div>
  <% end %>
</section>
